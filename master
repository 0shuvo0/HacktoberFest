import java.util.*;
class sentence
{
    protected String str;
    sentence()
    {
        str="";
    }
    void accept()
    {
        Scanner in =new Scanner(System.in);
        System.out.println("Enter Name");
        str=in.nextLine();
    }
    void display()
    {
        System.out.println(str);
    }
}
// end of base class 
