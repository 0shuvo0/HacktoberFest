import java.util.*;
class cuboid extends rect 
{
   int ht;
   void input2()
   {
        Scanner in =new Scanner (System .in);
        System.out.println("ENTER HEIGTH OF  THE CUBOID");
        ht=in.nextInt();
   }
   void volume()
    {
        int v;
        v=area()*ht;
        System.out.println("VOLUME ="+v);
    }
   public static void main()
   {
        cuboid obj= new cuboid ();
        obj. input1();
        obj.input2();
        obj.volume();
   }
}

    
        
        
        
        
